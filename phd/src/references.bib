  note      = {Publisher: National Academy of Sciences
               Section: Physical Sciences},
  keywords  = {machine learning, turbulence, computational physics, nonlinear partial differential equations},
  file      = {Full Text PDF:/Users/dion/Seafile/library/zotero/storage/VWEDEY2W/Kochkov et al. - 2021 - Machine learning–accelerated computational fluid d.pdf:application/pdf;Snapshot:/Users/dion/Seafile/library/zotero/storage/BGM6DFRH/e2101784118.html:text/html}
