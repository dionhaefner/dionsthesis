  number   = {11},
  urldate  = {2021-06-28},
  journal  = {Journal of Climate},
  author   = {Kretschmer, Marlene and Coumou, Dim and Donges, Jonathan F. and Runge, Jakob},
  month    = jun,
  year     = {2016},
  note     = {Publisher: American Meteorological Society
              Section: Journal of Climate},
  pages    = {4069--4081},
  file     = {Full Text PDF:/Users/dion/Seafile/library/zotero/storage/LWNE4WSS/Kretschmer et al. - 2016 - Using Causal Effect Networks to Analyze Different .pdf:application/pdf;Snapshot:/Users/dion/Seafile/library/zotero/storage/9RJHQWDL/jcli-d-15-0654.1.html:text/html}
}


